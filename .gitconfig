[merge]
	keepBackup = false
[mergetool]
	keepBackup = false
[difftool]
	prompt = false
[color]
	ui = auto
[core]
	autocrlf = input
	editor = /usr/bin/vim
	excludesfile = ~/.gitignore
[branch]
	autosetuprebase = always
[push]
	default = current
